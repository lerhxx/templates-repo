{
    "manifest_version": 2,
  
    "name": "{{projectName}}",
    "description": "{{projectDescription}}",
    "version": "{{projectVersion}}",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }